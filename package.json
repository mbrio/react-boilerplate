{
  "name": "react-marty-webpack-boilerplate",
  "version": "0.12.0",
  "engines": {
    "node": "~0.10"
  },
  "description": "A boilerplate for use compiling static applications consisting of ES6 source using Babel and Webpack. Comes pre-configured to use React, React Router, Marty, Bootstrap, and LESS.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build",
    "lint": "jsxhint 'src/**/*.jsx' 'src/*.jsx'",
    "build:development": "webpack --config webpack.development.config.js",
    "build:production": "webpack --config webpack.production.config.js",
    "prebuild:development": "npm run lint",
    "prebuild:production": "npm run lint",
    "build": "npm run build:development",
    "start": "webpack-dev-server -c -d -h --progress --config webpack.development.config.js"
  },
  "keywords": [
    "webpack",
    "react",
    "reactjs",
    "marty",
    "martyjs",
    "react-router",
    "babel",
    "bootstrap",
    "es6"
  ],
  "author": {
    "name": "Michael Diolosa",
    "email": "michael.diolosa@gmail.com",
    "url": "https://twitter.com/mbrio"
  },
  "contributors": [],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:mbrio/react-marty-webpack-boilerplate.git"
  },
  "homepage": "https://github.com/mbrio/react-marty-webpack-boilerplate",
  "bugs": "https://github.com/mbrio/react-marty-webpack-boilerplate/issues",
  "devDependencies": {
    "babel-core": "^4.7.1",
    "babel-loader": "^4.1.0",
    "bootstrap": "^3.3.2",
    "classnames": "^1.1.4",
    "css-loader": "^0.9.1",
    "file-loader": "^0.8.1",
    "jshint": "^2.6.3",
    "jsxhint": "^0.12.1",
    "jsxhint-loader": "^0.2.0",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "marty": "^0.8.14",
    "react": ">=0.12.2",
    "react-hot-loader": "^1.1.7",
    "react-router": "^0.12.4",
    "react-tools": "^0.12.2",
    "script-loader": ">=0.6.1",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {}
}
